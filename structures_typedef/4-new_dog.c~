#include "main.h"
#include "dog.h"
#include <stdlib.h>

dog_t *new_dog(char *name, float age, char *owner)
{
	dog_t *ptr;

	if (name == NULL || name == "")
	{
		ptr->name = "(nil)";
	}
	else
		ptr->name = name;
	ptr->age = age;
	if (owner == NULL || owner == "")
	{
		ptr->owner = "(nil)";
	}
	else
		ptr->age = age;

	return (ptr);
}
