#include <stdio.h>

int main(int argc, char *argv[])
{
	int change;
	int cent[5] = [25, 10, 5, 2, 1];
	int i = 0;

	if (argc != 2)
	{
		printf("Error\n");
		return (1);
	}
	if (atoi(argv[1]) <= 0)
	{
		printf("0\n");
		return (0);
	}
	change = cent_calc(argv[1], cent[], i);
	printf("%d\n", change);
