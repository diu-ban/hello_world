#include <stdlib.h>
#include <stdio.h>

int main(int argc, char *argv[])
{
	if (argc == 1)
	{
		printf("0\n");
		return (0);
	}

	int sum = 0;
	int i;

	for (i = 1; i < argc; i++)
	{
		if(atoi(argv[i]))
		{
			sum += atoi(argv[i]);
		}
		printf("Error\n");
		return (1);
	}
	printf("%d\n", sum);
	return (0);
}
