#include "main.h"
#include <stdlib.h>

char *string_nconcat(char *s1, char *s2, unsigned int n)
{
	unsigned int i = 0, y = 0;
	unsigned int size;
	char *str;

	if (s1 == NULL)
	{
		s1 = "";
	}
	if (s2 == NULL)
	{
		s2 = "";
	}
	while (*s1[i] != '\0')
	{
		i++;
	}
	while (*s2[y] != '\0')
	{
		y++;
	}
	if (n >= y)
	{
		str = malloc(sizeof(char) * (1 + i + y));
		size = 1 + i + y;
	}
	str = malloc(sizeof(char) * (1 + i + n));
	if (str == NULL)
	{
		return (NULL);
	}
	size = 1 + i + n;
	for (i = 0; i < size; i++)
	{
	  str[size] = 
	}
}
